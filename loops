#include <iostream>

using namespace std;

int main(void){
    cout << "hello" << endl;
    int num = 0; 
    while (num < 1000000){
        if (num % 1000 == 0){
            cout << "num = " << num << endl;
        }
        num++;
    }
    do {cout << "hello";} 
    while (num < 1000);
    
    for (int num = 0; num < 100; num++){
        cout << " num = " << num  << endl;
    }

    return 0;

}
